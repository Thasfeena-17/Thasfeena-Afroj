1//POSITIVE OR NEGATIVE

#include <stdio.h>
int main()
{
	int num;
	printf("enter the number");
	scanf("%d",&num);
	if(num<0)
	printf("number is negative");
	else if(num>0)
	printf("number is positive");
	else
	printf("zero");
	}

2//ODD OR EVEN

#include <stdio.h>
int main()
{
	int num;
	printf("enter the number");
	scanf("%d",&num);
	if(num%2==0)
	printf("number is even");
	else 
	printf("number is odd");
	
	}

3//SUM OF NUMBERS IN A GIVEN RANGE

#include <stdio.h>
int main()
{ 
    int a,b,i;
    int sum=0;
    printf("enter the value of a and b");
    scanf("%d%d",&a,&b);
    for(i=a;i<=b;i++)
    sum+=i;
    printf("the sum od numbers between the range %d and %d:%d",a,b,sum);
}

4//GREATEST OF TWO NUMBER
#include <stdio.h>
int main()
{
	int num1,num2;
	printf("enter the number1 and number2");
	scanf("%d%d",&num1,&num2);
	if(num1>num2)
	printf("number1 is greater");
	else 
	printf("number2 is greater");
	
}

5//GREATEST OF THREE NUMBERS

#include <stdio.h>
int main()
{
	int a,b,c;
	printf("enter the number1 ,number2 and number3");
	scanf("%d%d%d",&a,&b,&c);
	if(a>b && a>c)
	printf("%d is greater",a);
	else if(b>a && b>c)
	printf("%d is greater",b);
	else 
	printf("%d is greater",c);
	}

6//LEAP YEAR OR NOT

#include <stdio.h>
int main()
{
	int year;
	printf("enter the year");
	scanf("%d",&year);
	if(year%4==0)
	printf("%d is leap year",year);
	else if(year%400==0 && year%100!=0)
	printf("%d is leap year",year);
	else 
	printf("%d is not leap year",year);
	}

7//SUM OF NATURAL NUMBERS

#include <stdio.h>
int main()
{
	int n,i;
	int sum=0;
	printf("enter the n value");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	sum+=i;
	printf("sum of 	%d numbers is :%d",n,sum);
	return 0;
	}

8//PRIME NUMBERS

int main()
{ 
    int count=0,num,i;
    printf("enter the num");
    scanf("%d",&num);
    for(i=1;i<=num;i++)
    {
        if(num%i==0)
        count+=1;
}
    if( count > 2)
    printf("%d is not prime number",num);
    else
    printf("%d is  prime number",num);
    return 0;
}

9//SUM OF DIGITS OF GIVEN NUMBER

#include <stdio.h>
int main()
{ 
    int sum=0,num,i,r;
    printf("enter the num");
    scanf("%d",&num);
    while(num!=0){
    r=num%10;
    sum=sum+r;
    num=num/10;
    }
    printf("the sum of digits is:%d",sum);
    return 0;
}

